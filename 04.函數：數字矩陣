import java.util.Scanner;
public class p1 {
    public static void main(String[] args) {
        Scanner scn=new Scanner(System.in);
        int a=scn.nextInt();
        int b=scn.nextInt();
        F(a,b);
    }
  public  static  void  F(int w,int h){
        for(int i=1;i<=h;i++){
            for(int x=1;x<=w;x++){
                System.out.print(i*x+"\t");
            }
            System.out.println();
        }
  }
}
