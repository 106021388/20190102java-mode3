import java.util.Arrays;
import java.util.Scanner;
public class p4 {
    public static void main(String[] args) {
        Scanner scn=new Scanner(System.in);
        int a=scn.nextInt();
        int b[]=new int [a];
        for(int i=0;i<a;i++){
            b[i]=scn.nextInt();
        }
        Arrays.sort(b);
        for(int i=0;i<a;i++){
            System.out.println(b[i]);
        }
    }
}
