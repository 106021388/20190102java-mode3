import java.util.Scanner;
public class p4 {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        String a = scn.next();
        String b = "";

        for (int i=a.length()-1;i>=0;i--){
            b+=a.charAt(i);
        }
       System.out.println(b);

    }
