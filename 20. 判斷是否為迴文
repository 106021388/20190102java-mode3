import java.util.Scanner;

public class p3 {
    public static void main(String[] args) {
        //迴文
        Scanner scn=new Scanner(System.in);
        String a=scn.next();
        String b="";
        for(int i=a.length()-1;i>=0;i--){
            b+=a.charAt(i);
            }
            if(a.compareTo(b)==0){
                System.out.println("Yes");
            }else{
                System.out.println("No");
            }
    }
}
